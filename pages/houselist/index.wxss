/* pages/houselist/index.wxss */
page::after {
  background: #fff;
}
.van-tabs__nav .van-tabs__line {
  display: none;
}
.van-tabs__nav .van-ellipsis {
  position: relative;
}

.van-tabs__nav .van-tab.van-ellipsis::before {
  content: '';
  position: absolute;
  right: 10px;
  top:50%;
  transform: translateY(-50%);
  width: 20px;
  height: 20px;
  background-image: url("data:image/png;base64,");
  /* background-image: url("data:image/png;base64,"); */
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
  width: 26px;
  height: 34px;
  right: -6px;
}

.house-list  {
  padding: 110px 16px;
}
.house-list .null-message {
  padding-top: 50px;
  font-size: 14px;
  text-align: center;
  color: #999;
}